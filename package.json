{
  "name": "Gradeflix",
  "version": "0.0.1",
  "description": "Advanced Website - Midterm",
  "keywords": [
    "loopback-application",
    "loopback"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": "^14.0"
  },
  "scripts": {
    "build": "lb-tsc",
    "build:watch": "lb-tsc --watch",
    "lint": "yarn run eslint && yarn run prettier:check",
    "lint:fix": "yarn run eslint:fix && yarn run prettier:fix",
    "prettier:cli": "lb-prettier \"**/*.ts\" \"**/*.js\"",
    "prettier:check": "yarn run prettier:cli -l",
    "prettier:fix": "yarn run prettier:cli --write",
    "eslint": "lb-eslint --report-unused-disable-directives .",
    "eslint:fix": "yarn run eslint --fix",
    "pretest": "yarn run rebuild",
    "test": "lb-mocha --allow-console-logs \"dist/__tests__\"",
    "posttest": "yarn run lint",
    "test:dev": "lb-mocha --allow-console-logs dist/__tests__/**/*.js && yarn run posttest",
    "docker:build": "docker build -t ces_om .",
    "docker:run": "docker run -p 3000:3000 -d ces_om",
    "premigrate": "yarn run build",
    "migrate": "node ./dist/migrate",
    "preopenapi-spec": "yarn run build",
    "openapi-spec": "node ./dist/openapi-spec",
    "prestart": "yarn run rebuild",
    "start": "node -r source-map-support/register .",
    "start:watch": "yarn run rebuild && tsc-watch --target es2017 --outDir ./dist --onSuccess \"node .\"",
    "clean": "lb-clean dist *.tsbuildinfo .eslintcache",
    "rebuild": "yarn run clean && yarn run build"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "Khoa Nguyen <18127121@student.hcmus.edu.vn>",
  "license": "",
  "files": [
    "README.md",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "dependencies": {
    "@loopback/authentication": "^7.3.4",
    "@loopback/authentication-jwt": "^0.9.4",
    "@loopback/authorization": "^0.7.5",
    "@loopback/boot": "^3.1.2",
    "@loopback/core": "^2.13.1",
    "@loopback/repository": "^3.3.0",
    "@loopback/rest": "^9.1.1",
    "@loopback/rest-explorer": "^3.0.5",
    "@loopback/service-proxy": "^3.0.5",
    "@types/ejs": "^3.1.0",
    "@types/html-to-text": "^8.0.1",
    "@types/multer": "^1.4.7",
    "@types/socket.io": "^3.0.2",
    "axios": "^0.21.4",
    "bcryptjs": "^2.4.3",
    "crypto-js": "^4.1.1",
    "csvtojson": "^2.0.10",
    "dayjs": "^1.10.7",
    "dotenv": "^10.0.0",
    "dotenv-expand": "^5.1.0",
    "ejs": "^3.1.6",
    "google-auth-library": "^7.10.2",
    "html-to-text": "^8.1.0",
    "jsonwebtoken": "^8.5.1",
    "juice": "^8.0.0",
    "loopback-connector-postgresql": "^5.3.0",
    "loopback-connector-rest": "^3.2.0",
    "moment": "^2.29.1",
    "multer": "^1.4.3",
    "nanoid": "^3.1.30",
    "nodemailer": "^6.7.0",
    "socketio": "^1.0.0",
    "tslib": "^2.0.0",
    "xlsx": "^0.17.4"
  },
  "devDependencies": {
    "@loopback/build": "^6.2.8",
    "@loopback/eslint-config": "^10.0.4",
    "@loopback/testlab": "^3.2.10",
    "@types/crypto-js": "^4.0.2",
    "@types/jest": "^26.0.20",
    "@types/jsonwebtoken": "8.5.1",
    "@types/lodash": "^4.14.168",
    "@types/md5": "^2.3.0",
    "@types/mocha": "^8.0.4",
    "@types/node": "14.17.3",
    "@types/nodemailer": "^6.4.4",
    "eslint": "^7.15.0",
    "source-map-support": "^0.5.19",
    "tsc-watch": "^4.5.0",
    "typescript": "~4.1.2"
  }
}
